ength - 1 ? 'last' : undefined\">\n    <td>{{ index}}</td>\n        <td>{{ petDetail.urlName }}</td>\n        <td style=\"width: 30%\">\n          <img :src=\"petDetail.url\" />\n        </td>\n        <td>{{ petDetail.ratingValue }}</td>\n        <td v-if=\"petDetail.ratingValue > 2\"> Cute</td>\n        <td v-else>Not cute</td>\n        <!-- <td> <StarRating :key =\"index\"></StarRating></td> -->\n      </tr>\n    </table>\n  \n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"Rating\",\n  props: {\n    petDetails: { type:Array}\n\n  },\n  data() {\n    return {\n      page: 1,\n    //   petDetails: Array\n    //     .fill()\n    //     .map((_, i) => i),\n      observer: undefined,\n      limit:10,\n    };\n  },\n  methods: {\n    async addObserver() {\n      await this.$nextTick();\n      const options = {\n        root: document,\n        rootMargin: \"20px\",\n        threshold: 1,\n      };\n setTimeout(e => {\n      const callback = (entries) => {\n\n        if (entries.isIntersecting) {\n            \n        if(this.limit !== this.petDetails.length)\n            {\n              this.page++;\n              this.limit= this.limit+10;\n            }\n     \n            \n        }\n      };\n       }, 200);\n      this.observer = new IntersectionObserver(callback, options);\n      this.observer.observe(this.$refs.last);\n    },\n  },\n  mounted() {\n    this.addObserver();\n  },\n  watch: {\n    arr: {\n      deep: true,\n      handler() {\n        this.addObserver();\n      },\n    },\n  },\n};\n</script>\n\n<style>\n.list-group-wrapper {\n  position: relative;\n}\n.list-group {\n  overflow: auto;\n  height: 50vh;\n  /* border: 2px solid #dce4ec;\n  border-radius: 5px; */\n}\nimg {\n  margin-top: 5px;\n  max-width: 50%;\n  max-height: 500px;\n}\ntable {\n  font-family: arial, sans-serif;\n  border-collapse: collapse;\n  width: 100%;\n  margin: auto;\n}\n\ntd,\nth {\n  border: 1px solid #dddddd;\n  text-align: left;\n  padding: 8px;\n}\n\ntr:nth-child(even) {\n  background-color: #dddddd;\n}\n</style>"]}]}