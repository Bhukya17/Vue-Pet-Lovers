AAa,IAAb,EAAa,IAAb,EAAI,MAAJ,CAEG,E,aADH,YAAA,CAAoB,IAApB,EAAoB,IAApB,EAAI,aAAJ,CACG,CALL,E;;AAAA,C;;;AAQM,EAAA,KAAkB,EAAlB;AAAA,aAAA;AAAA;;;;;;;;;sDAtBZ,U,EAKA,YAAA,CA+BM,KA/BN,EAAA,UAAA,EA+BM,CA7BJ,YAAA,CA2BM,KA3BN,EAAA,UAAA,EA2BM,CA1BJ,YAAA,CAIa,WAJb,EAIa;AAJD,IAAA,IAAI,EAAC;AAIJ,GAJb,EAAuB;wBACrB;AAAA,aAEM,C,gBAFN,YAAA,CAEM,KAFN,EAAA,UAAA,EAEM,CADJ,UACI,E,UAAA,CAFN,E;;AAAA,O,YAA6B,KAAA,CAAA,O,GAEvB,CAFN;AAAA,K,CADqB;;;;AAAA,GAAvB,CA0BI,EArBJ,YAAA,CAiBQ,OAjBR,EAAA,UAAA,EAiBQ,CAhBN,UAgBM,G,kBAVN,YAAA,CASK,SATL,EASK,IATL,EASK,WAAA,CATmD,MAAA,CAAA,UASnD,EAT6D,UAA/B,SAA+B,EAApB,KAAoB,EAAf;yBAAnD,YAAA,CASK,IATL,EASK;AATD,MAAA,IAAI,EAAC,iBASJ;AATuE,MAAA,GAAG,EAAE;AAS5E,KATL,E,CACE,YAAA,CAAgC,IAAhC,EAAgC,IAAhC,EAAgC,gBAAA,CAAzB,SAAS,CAAC,OAAe,CAAhC,EAAwB;AAAA;AAAxB,K,EACA,YAAA,CAEK,IAFL,EAAA,UAAA,EAEK,CADH,YAAA,CAA4B,KAA5B,EAA4B;AAAtB,MAAA,GAAG,EAAE,SAAS,CAAC;AAAO,KAA5B,E,IAAA,E;;AAAA,M,OAAA,CACG,CAFL,C,EAGA,YAAA,CAAoC,IAApC,EAAoC,IAApC,EAAoC,gBAAA,CAA7B,SAAS,CAAC,WAAmB,CAApC,EAA4B;AAAA;AAA5B,K,EACU,SAAS,CAAC,WAAV,GAAqB,C,kBAA/B,YAAA,CAA+C,IAA/C,EAA+C,WAA/C,EAAqC,OAArC,C,mBACA,YAAA,CAAwB,IAAxB,EAAwB,WAAxB,EAAW,UAAX,C,GACA,mBAAA,CAAA,uDAAA,C,CARF,C;GASK,CATL,E;;AAAA,GAUM,EAjBR,CAqBI,EAHJ,mBAAA,CAAA,oKAAA,CAGI,CA3BN,CA6BI,CA/BN,C","sourcesContent":["<template>\n\n<div class=\"heading\">\n  <h1>Infinite Scroll</h1>\n  <h4>A simple infinite scroll example using Vue.js</h4>\n</div>\n\n<div class=\"container\" id=\"app\">\n  \n  <div class=\"list-group-wrapper\">\n    <transition name=\"fade\">\n      <div class=\"loading\" v-show=\"loading\">\n        <span class=\"fa fa-spinner fa-spin\"></span> Loading\n      </div>\n    </transition>\n    <table class=\"list-group\" id=\"infinite-list\">\n      <tr>\n        <th>Image Name</th>\n        <th>Image</th>\n        <th>Rate</th>\n        <th>Cute-or-Not</th>\n      </tr>\n      <tr lass=\"list-group-item\" v-for=\"(petDetail, index) in petDetails\"  v-bind:key=\"index\">\n        <td>{{ petDetail.urlName }}</td>\n        <td style=\"width: 30%\">\n          <img :src=\"petDetail.url\" />\n        </td>\n        <td>{{ petDetail.ratingValue }}</td>\n        <td v-if=\"petDetail.ratingValue > 2\"> Cute</td>\n        <td v-else>Not cute</td>\n        <!-- <td> <StarRating :key =\"index\"></StarRating></td> -->\n      </tr>\n    </table>\n    <!-- <ul class=\"list-group\" id=\"infinite-list\">\n      <li class=\"list-group-item\" v-for=\"item in items\" v-text=\"item\" v-bind:key=\"item\"></li>\n    </ul> -->\n  </div>\n  \n</div>\n\n  \n</template>\n\n<script>\n\nexport default {\n  name: \"InfiniteScroll\",\n  props: {\n    petDetails: {\n      type: Array,\n      required: true,\n    },\n  },\n  data() {\n      return{\n    loading: false,\n    nextItem: 1,\n    items: []\n      }\n  },\n  mounted () {\n\n    // Detect when scrolled to bottom.\n    const listElm = document.querySelector('#infinite-list');\n    listElm.addEventListener('scroll', e => {\n      if(listElm.scrollTop + listElm.clientHeight >= listElm.scrollHeight) {\n        this.loadMore();\n      }\n    });\n\n    // Initially load some items.\n    this.loadMore();\n\n  },\n  methods: {\n    loadMore () {\n      \n      /** This is only for this demo, you could \n        * replace the following with code to hit \n        * an endpoint to pull in more data. **/\n      this.loading = true;\n      setTimeout(e => {\n        for (var i = petDetail.length; i < 5; i++) {\n          this.items.push('Item ' + this.nextItem++);\n        }\n        this.loading = false;\n      }, 200);\n      /**************************************/\n      \n    }\n  }\n  \n\n};\n</script>\n\n<style >\n$purple: #5c4084;\n\nbody {\n  background-color: $purple;\n  padding: 50px;\n}\n.container {\n  padding: 40px 80px 15px 80px;\n  background-color: #fff;\n  border-radius: 8px;\n  max-width: 800px;\n}\n.heading {\n  text-align: center;\n  h1 {\n    background: -webkit-linear-gradient(#fff, #999);\n    -webkit-text-fill-color: transparent;\n    -webkit-background-clip: text;\n    text-align: center;\n    margin: 0 0 5px 0;\n    font-weight: 900;\n    font-size: 4rem;\n    color: #fff;\n  }\n  h4 {\n    color: lighten(#5c3d86,30%);\n    text-align: center;\n    margin: 0 0 35px 0;\n    font-weight: 400;\n    font-size: 24px;\n  }\n}\n\n.list-group-wrapper {\n  position: relative;\n}\n.list-group {\n  overflow: auto;\n  height: 50vh;\n  border: 2px solid #dce4ec;\n  border-radius: 5px;\n}\n.list-group-item {\n  margin-top: 1px;\n  border-left: none;\n  border-right: none;\n  border-top: none;\n  border-bottom: 2px solid #fff;\n}\n.loading {\n  text-align: center;\n  position: absolute;\n  color: #fff;\n  z-index: 9;\n  background: $purple;\n  padding: 8px 18px;\n  border-radius: 5px;\n  left: calc(50% - 45px);\n  top: calc(50% - 18px);\n}\n\n.fade-enter-active, .fade-leave-active {\n  transition: opacity .5s\n}\n.fade-enter, .fade-leave-to {\n  opacity: 0\n}\nimg {\n  margin-top: 5px;\n  max-width: 50%;\n  max-height: 500px;\n}\ntable {\n  font-family: arial, sans-serif;\n  border-collapse: collapse;\n  width: 50%;\n  margin: auto;\n}\n\ntd,\nth {\n  border: 1px solid #dddddd;\n  text-align: left;\n  padding: 8px;\n}\n\ntr:nth-child(even) {\n  background-color: #dddddd;\n}\n</style>\n"],"sourceRoot":""}]}