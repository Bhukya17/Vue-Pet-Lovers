st callback = (entries) => {\n        setTimeout((e) => {\n          if (entries.isIntersecting) {\n            if (this.limit !== this.petDetails.length) {\n              this.page++;\n              this.limit = this.limit + 10;\n            }\n          }\n        }, 1000);\n      };\n\n      this.observer = new IntersectionObserver(callback, options);\n      this.observer.observe(this.$refs.last);\n    },\n  },\n  mounted() {\n    this.addObserver();\n  },\n  watch: {\n    arr: {\n      deep: true,\n      handler() {\n        this.addObserver();\n      },\n    },\n  },\n};\n</script>\n\n<style>\n.list-group-wrapper {\n  position: relative;\n}\n.list-group {\n  overflow: auto;\n  height: 50vh;\n  /* border: 2px solid #dce4ec;\n  border-radius: 5px; */\n}\nimg {\n  margin-top: 5px;\n  max-width: 50%;\n  max-height: 500px;\n}\ntable {\n  font-family: arial, sans-serif;\n  border-collapse: collapse;\n  width: 100%;\n  margin: auto;\n}\n\ntd,\nth {\n  border: 1px solid #dddddd;\n  text-align: left;\n  padding: 8px;\n}\n\ntr:nth-child(even) {\n  background-color: #dddddd;\n}\n</style>\n"],"sourceRoot":""}]}